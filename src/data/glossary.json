  "interface_concepts": [
    {
      "term": "Fusion Label",
      "traditional_equivalent": "Zodiac Sign",
      "description": "Poetic name representing the blending of house and sign qualities",
      "examples": ["Seed of Flame", "Body of Soil", "Breath of Stories"],
      "user_definition": ""
    },
    {
      "term": "Archetypal Figure",
      "traditional_equivalent": "Planet",
      "description": "The symbolic character embodying a planet's essence",
      "examples": ["The Radiant One", "The Nurturer", "The Warrior"],
      "user_definition": ""
    },
    {
      "term": "House Field",
      "traditional_equivalent": "House",
      "description": "Life arena or domain where planetary energies express",
      "examples": ["House of Emergence", "House of Deep Transformation"],
      "user_definition": ""
    },
    {
      "term": "Cusp",
      "description": "The boundary or threshold between two houses/signs",
      "context": "Planets near cusps blend qualities of adjacent houses",
      "user_definition": ""
    },
    {
      "term": "Orb",
      "description": "The allowable degree range for an aspect to be considered active",
      "context": "Different aspects have different orbs (e.g., conjunction: 10°, sextile: 6°)",
      "user_definition": ""
    },
    {
      "term": "Zipline",
      "description": "Cinematic camera movement gliding through multiple house layers",
      "context": "Used when navigating between non-adjacent houses",
      "user_definition": ""
    },
    {
      "term": "Star Cues",
      "description": "Floating poetic phrases within each house layer",
      "context": "Contextual sentences that change based on view mode",
      "user_definition": ""
    },
    {
      "term": "Fluid Astrology",
      "description": "Non-deterministic, relational approach emphasizing archetypal fluidity over fixed meanings",
      "philosophy": "Anti-essentialist, honors cultural origins, avoids prediction",
      "user_definition": ""
    }
  ],

  "technical_terms": [
    {
      "term": "Helical Spiral",
      "description": "The 3D spiral structure with 12 layers representing the houses",
      "technical_detail": "Grows from radius 2.5 to 5.5, height 26 units",
      "user_definition": ""
    },
    {
      "term": "Ecliptic Degree",
      "description": "A planet's position measured in degrees around the 360° zodiac wheel",
      "context": "Used to determine house placement and aspects",
      "user_definition": ""
    },
    {
      "term": "Modality",
      "description": "Quality of action: Cardinal (initiating), Fixed (sustaining), or Mutable (adapting)",
      "user_definition": ""
    },
    {
      "term": "Polarity",
      "description": "Yang (active, masculine) or Yin (receptive, feminine) energy",
      "user_definition": ""
    },
    {
      "term": "Element",
      "description": "Fire (spirit), Earth (body), Air (mind), or Water (emotion)",
      "user_definition": ""
    },
    {
      "term": "Ascendant",
      "description": "The zodiac sign rising on the eastern horizon at birth; marks the 1st house cusp",
      "also_known_as": "Rising Sign",
      "user_definition": ""
    },
    {
      "term": "Natal Chart",
      "description": "A map of planetary positions at the exact moment and location of birth",
